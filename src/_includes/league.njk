---
layout: base.njk
---
<div class="container">
    <div class="jumbotron">
        <h1>{{sponsor}}</h1>
        <h2>{{ title }}</h2>
    </div>
    <div class="row">
        <div class="col">
            {% set league = title | slug %}
            <ul>
            {% for club in collections[league] | reverse %}
                <li><a href="/club/{{ club.data.orgname | slug }}">{{ club.data.orgname }}</a></li>
            {% endfor %}
            </ul>
        </div>
    </div>
</div>


